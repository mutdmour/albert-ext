{
    "manifest_version": 2,
    "name": "Chrome-vue-boilerplate",
    "version": "0.1.0",
    "description": "Build an awesome Chrome extension",
		"background": {
			"scripts": ["background.js"]
		},
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "views/popup.html"
    }
}